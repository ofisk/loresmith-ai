{
  "name": "cloudflare-agent-starter",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ofisk/loresmith-ai"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "vite dev",
    "dev": "wrangler dev --config wrangler.local.jsonc --port 8787 --local",
    "dev:cloudflare": "wrangler dev --config wrangler.dev.jsonc --port 8787",
    "dev:setup": "./scripts/setup-dev.sh",
    "build": "vite build",
    "deploy": "vite build && wrangler deploy --config wrangler.jsonc",
    "deploy:dev": "vite build && wrangler deploy --config wrangler.dev.jsonc",
    "migrate": "./scripts/migrate.sh",
    "migrate:dev": "wrangler d1 migrations apply loresmith-db-dev --config wrangler.dev.jsonc",
    "test": "vitest",
    "types": "wrangler types",
    "format": "prettier --write .",
    "check": "prettier . --check && npx @biomejs/biome lint src tests && tsc",
    "validate": "npm run check && npm test",
    "prepare": "husky"
  },
  "keywords": [
    "cloudflare",
    "ai",
    "agents"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.899.0",
    "@cloudflare/vite-plugin": "1.9.4",
    "@cloudflare/vitest-pool-workers": "^0.8.53",
    "@cloudflare/workers-types": "^4.20250715.0",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "multer": "^2.0.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "wrangler": "^4.30.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/react": "^1.2.12",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@types/marked": "^6.0.0",
    "agents": "^0.0.106",
    "ai": "^4.3.19",
    "aws4fetch": "^1.0.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.8.5",
    "jose": "^6.0.11",
    "lucide-react": "^0.545.0",
    "marked": "^16.0.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.54",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "overrides": {
